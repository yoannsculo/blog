---
layout: default
---

<div class="container">
	<div class="well">
	{% set year = '0' %}
	{% for post in site.posts|reverse %}
		{% if post.date|yearFormat != year %}
			{% set year = post.date|yearFormat %}
			<h2>{{ year }}</h2>
		{% endif %}
		<div style="margin-bottom:5px;"><span class="label label-info">{{ post.date|dateFormat }}</span> Â» <a href="{{ post.url }}">{{ post.title }}</a><br></div>
	{% endfor %}
	</div>
</div>
