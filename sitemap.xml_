	xmlns:video="http://www.sitemaps.org/schemas/sitemap-video/1.1">

	<url>
		<loc>{{ blog.url }}</loc>
		<lastmod>{{ site.now|xmldatetime() }}</lastmod>
		<changefreq>daily</changefreq>
		<priority>1.0</priority>
	</url>

