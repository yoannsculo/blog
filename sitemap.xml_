<?xml version="1.0" encoding="UTF-8"?>

<?xml-stylesheet type="text/xsl" href="sitemap.xsl"?>

<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
	xmlns:image="http://www.sitemaps.org/schemas/sitemap-image/1.1"
	xmlns:video="http://www.sitemaps.org/schemas/sitemap-video/1.1">

	<url>
		<loc>{{ blog.url }}</loc>
		<lastmod>{{ site.now|xmldatetime() }}</lastmod>
		<changefreq>daily</changefreq>
		<priority>1.0</priority>
	</url>

	{% for post in site.posts|reverse %}
	<url>
		<loc>{{ blog.url }}/{{ post.url }}</loc>
		<lastmod>{{ post.date }}</lastmod>
		<changefreq>daily</changefreq>
		<priority>0.8</priority>
	</url>
	{% endfor %}

	<url>
		<loc>{{blog.url}}/Yoann_Sculo_CV.pdf</loc>
		<lastmod>{{ site.now|xmldatetime() }}</lastmod>
		<changefreq>daily</changefreq>
		<priority>1.0</priority>
	</url>
	<url>
		<loc>{{blog.url}}/Yoann_Sculo_CV_Linux_Embarque.pdf</loc>
		<lastmod>{{ site.now|xmldatetime() }}</lastmod>
		<changefreq>daily</changefreq>
		<priority>1.0</priority>
	</url>
	<url>
		<loc>{{blog.url}}/Yoann_Sculo_CV_Linux_Embarque.odt</loc>
		<lastmod>{{ site.now|xmldatetime() }}</lastmod>
		<changefreq>daily</changefreq>
		<priority>1.0</priority>
	</url>
	<url>
		<loc>{{blog.url}}/Yoann_Sculo_CV_Linux_Embarque.doc</loc>
		<lastmod>{{ site.now|xmldatetime() }}</lastmod>
		<changefreq>daily</changefreq>
		<priority>1.0</priority>
	</url>
	<url>
		<loc>{{blog.url}}/Yoann_Sculo_Resume_Embedded_Linux.pdf</loc>
		<lastmod>{{ site.now|xmldatetime() }}</lastmod>
		<changefreq>daily</changefreq>
		<priority>1.0</priority>
	</url>
</urlset>

